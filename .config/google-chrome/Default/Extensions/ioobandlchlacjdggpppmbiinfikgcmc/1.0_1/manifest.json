{
   "background": {
      "persistent": true,
      "scripts": [ "src/bg/background.js" ]
   },
   "browser_action": {
      "default_icon": "icons/icon19.png",
      "default_title": "Set custom id"
   },
   "content_scripts": [ {
      "js": [ "src/inject/inject.js" ],
      "matches": [ "*://*/*" ],
      "run_at": "document_end"
   } ],
   "default_locale": "en",
   "description": "Runs SiteGainer editor on any website without implementing it in source code",
   "homepage_url": "http://sitegainer.com",
   "icons": {
      "128": "icons/icon128.png",
      "16": "icons/icon16.png",
      "48": "icons/icon48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "SiteGainer Editor",
   "permissions": [ "tabs", "contextMenus", "activeTab", "https://*/*", "http://*/*" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.0"
}
